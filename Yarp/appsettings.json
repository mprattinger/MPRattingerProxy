{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "svs": {
        "ClusterId": "SVSCluster",
        "Match": {
          "Path": "{**catch-all}",
          "Hosts": [ "svs.mprattinger.com" ]
        }
      },
      "portainer": {
        "ClusterId": "Portainer",
        "Match": {
          "Path": "{**catch-all}",
          "Hosts": [ "portainer.mprattinger.com" ]
        }
      },
      "all": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "SVSCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://10.0.0.5:7123"
          }
        }
      },
      "Portainer": {
        "Destinations": {
          "destination1": {
            "Address": "https://10.0.0.5:9443"
          }
        }
      },
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "https://www.orf.at"
          }
        }
      }
    }
  }
}
